{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["**/*.prisma", "prisma/**", "dev.db"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/index.js"
      }
    ],
    "buildCommand": "npx nx build api.minerva"
  }
  